                "description": "获得二级分类下的商品",
                "operationId": "getItemsBySubCategoryId (response Id or Object)",
                "parameters": [
                    {
                        "name": "subCategoryId",
                        "in": "path",
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        },
                        "description": "二级分类id",
                        "required": true,
                        "example": 1
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
