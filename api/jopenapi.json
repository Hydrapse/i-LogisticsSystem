            "Category": {
                "title": "Category",
                "description": "商品大类",
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int64",
                        "description": "大类id",
                        "example": 1
                    },
                    "name": {
                        "type": "string",
                        "description": "大类名称",
                        "example": "Food"
                    },
                    "description": {
                        "type": "string",
