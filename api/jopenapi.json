                    }
                }
            },
            "Product": {
                "title": "Product",
                "description": "商品",
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int64",
                        "description": "商品id",
                        "example": 1
                    },
                    "name": {
                        "type": "string",
                        "description": "商品名称",
                        "example": "CocaCola_330_c"
                    },
                    "provider": {
                        "type": "integer",
                        "format": "int64",
                        "description": "供应商id",
                        "example": 1
                    },
                    "price": {
                        "type": "number",
                        "format": "double",
                        "description": "价格",
                        "example": 3.0
                    },
                    "description": {
                        "type": "string",
                        "description": "商品描述",
                        "example": "可口可乐 330ml 罐装"
                    },
                    "atrribute": {
                        "type": "array",
                        "default": "商品属性",
                        "items": {
                            "type": "string"
                        },
                        "example": [
                            "红罐",
                            "可口"
                        ]
                    }
                }
            }
