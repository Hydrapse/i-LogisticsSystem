            "get": {
                "tags": [
                    "goods"
                ],
                "description": "获得二级分类对象的一级分类",
                "operationId": "getCategoryBySubCategoryId (response Id or Object)",
                "parameters": [
                    {
                        "name": "subCategoryId",
