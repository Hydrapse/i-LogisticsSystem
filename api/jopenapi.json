                        "application/json": {
                            "schema": {
                                "allOf": [
                                    {
                                        "$ref": "#/components/schemas/CategoryId"
                                    },
                                    {
                                        "$ref": "#/components/schemas/Category"
                                    }
                                ],
                                "required": [
                                    "categoryId"
                                ]
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "default": {
                        "description": "ERROR"
                    }
                }
            },
            "get": {
                "tags": [
                    "goods"
                ],
                "description": "获取商品一级分类",
                "operationId": "getCategory",
                "parameters": [
                    {
                        "name": "categoryId",
                        "in": "query",
                        "schema": {
                            "type": "integer",
