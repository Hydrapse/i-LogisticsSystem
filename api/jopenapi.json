            "delete": {
                "tags": [
                    "goods"
                ],
                "description": "删除商品一级分类",
                "operationId": "deleteCategory",
                "parameters": [
                    {
                        "name": "categoryId",
                        "in": "query",
                        "schema": {
                            "type": "integer",
                            "format": "int64"
